{
  "name": "bffapi",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./dist/server.js",
    "build": "tsc",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "test": "jest"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~4.1.1",
    "express": "~4.17.1",
    "log4js": "^6.3.0",
    "morgan": "~1.10.0",
    "uuid": "^8.2.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.6",
    "@types/jest": "^26.0.3",
    "@types/morgan": "^1.9.1",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "eslint": "^7.3.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "<rootDir>/test/**/*.(test).{js,jsx,ts,tsx}"
    ]
  }
}
